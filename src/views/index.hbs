                            <path d="M8 3.25a.75.75 0 01.75.75v3.25H11a.75.75 0 010 1.5H7.25V4A.75.75 0 018 3.25z">
                            </path>
                        </svg></span></div>
            </div>
                
               {{#each mus}}
                    {{>music title = title artist = artist album = album _id=_id}}
                    
               {{/each}}

               
              
            </div>
        </div>

